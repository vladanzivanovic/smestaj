<?php

namespace SiteBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Category
 *
 * @ORM\Table(name="category")
 * @ORM\Entity(repositoryClass="SiteBundle\Repository\CategoryRepository")
 */
class Category
{
    /**
     * @var integer
     *
     * @ORM\Column(name="Id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="Name", type="string", length=250, nullable=false)
     */
    private $name;

    /**
     * @var string
     *
     * @ORM\Column(name="Alias", type="string", length=250, nullable=false)
     */
    private $alias;

    /**
     * @var string
     *
     * @ORM\Column(name="Image", type="string", length=250, nullable=true)
     */
    private $image;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="SysCreatedTime", type="datetime", nullable=false)
     */
    private $syscreatedtime;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="SysModifyTime", type="datetime", nullable=false)
     */
    private $sysmodifytime;

    /**
     * @var User
     *
     * @ORM\ManyToOne(targetEntity="SiteBundle\Entity\User", inversedBy="Id")
     * @ORM\JoinColumn(name="SysCreatedUserId", referencedColumnName="Id")
     */
    private $syscreateduserid;

    /**
     * @var User
     *
     * @ORM\ManyToOne(targetEntity="SiteBundle\Entity\User", inversedBy="Id")
     * @ORM\JoinColumn(name="SysModifyUserId", referencedColumnName="Id")
     */
    private $sysmodifyuserid;

    /**
     * @ORM\OneToMany(targetEntity="SiteBundle\Entity\Ads", mappedBy="categoryid")
     */
    private $adsids;



    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set name
     *
     * @param string $name
     *
     * @return Category
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Set alias
     *
     * @param string $alias
     *
     * @return Category
     */
    public function setAlias($alias)
    {
        $this->alias = $alias;

        return $this;
    }

    /**
     * Get alias
     *
     * @return string
     */
    public function getAlias()
    {
        return $this->alias;
    }

    /**
     * Set image
     *
     * @param string $image
     *
     * @return Category
     */
    public function setImage($image)
    {
        $this->image = $image;

        return $this;
    }

    /**
     * Get image
     *
     * @return string
     */
    public function getImage()
    {
        return $this->image;
    }

    /**
     * Set syscreatedtime
     *
     * @param \DateTime $syscreatedtime
     *
     * @return Category
     */
    public function setSyscreatedtime($syscreatedtime)
    {
        $this->syscreatedtime = $syscreatedtime;

        return $this;
    }

    /**
     * Get syscreatedtime
     *
     * @return \DateTime
     */
    public function getSyscreatedtime()
    {
        return $this->syscreatedtime;
    }

    /**
     * Set sysmodifytime
     *
     * @param \DateTime $sysmodifytime
     *
     * @return Category
     */
    public function setSysmodifytime($sysmodifytime)
    {
        $this->sysmodifytime = $sysmodifytime;

        return $this;
    }

    /**
     * Get sysmodifytime
     *
     * @return \DateTime
     */
    public function getSysmodifytime()
    {
        return $this->sysmodifytime;
    }

    /**
     * Set syscreateduserid
     *
     * @param integer $syscreateduserid
     *
     * @return Category
     */
    public function setSyscreateduserid($syscreateduserid)
    {
        $this->syscreateduserid = $syscreateduserid;

        return $this;
    }

    /**
     * Get syscreateduserid
     *
     * @return integer
     */
    public function getSyscreateduserid()
    {
        return $this->syscreateduserid;
    }

    /**
     * Set sysmodifyuserid
     *
     * @param integer $sysmodifyuserid
     *
     * @return Category
     */
    public function setSysmodifyuserid($sysmodifyuserid)
    {
        $this->sysmodifyuserid = $sysmodifyuserid;

        return $this;
    }

    /**
     * Get sysmodifyuserid
     *
     * @return integer
     */
    public function getSysmodifyuserid()
    {
        return $this->sysmodifyuserid;
    }
    /**
     * Constructor
     */
    public function __construct()
    {
        $this->adsids = new \Doctrine\Common\Collections\ArrayCollection();
    }

    /**
     * Add adsid
     *
     * @param \SiteBundle\Entity\Ads $adsid
     *
     * @return Category
     */
    public function addAdsid(\SiteBundle\Entity\Ads $adsid)
    {
        $this->adsids[] = $adsid;

        return $this;
    }

    /**
     * Remove adsid
     *
     * @param \SiteBundle\Entity\Ads $adsid
     */
    public function removeAdsid(\SiteBundle\Entity\Ads $adsid)
    {
        $this->adsids->removeElement($adsid);
    }

    /**
     * Get adsids
     *
     * @return \Doctrine\Common\Collections\Collection
     */
    public function getAdsids()
    {
        return $this->adsids;
    }
}
